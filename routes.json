{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "platformErrorOverrides": [
        {
            "errorType": "NotFound",
            "serve": "/404"
        },
        {
            "errorType": "Unauthenticated",
            "statusCode": "302",
            "serve": "/login"
        }
    ],
    "mimeTypes": {
        "json": "application/json"
    }
}